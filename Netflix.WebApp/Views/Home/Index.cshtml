@using Netflix.WebApp.ViewModel
@model SeriesMovieViewModel
@{
    ViewData["Title"] = "Home Page";
}

<link href="~/lib/OwlCarousel2-2.3.4/dist/assets/owl.carousel.css" rel="stylesheet" type="text/css" />"
